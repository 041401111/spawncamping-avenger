package checkstrings;

/**
 *
 * @author 041401111
 */
public class CheckStrings {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        //declare variables
        String string1a;
        String string1b;
        
        //set the values
        string1a="hello";
        string1b="hello";
        
        System.out.println("are the first set the same and equal? " +(string1a==string1b)+" and " +(string1a.equals(string1b)));
        
        //now create two new objects and check the same
        String string2a;
        String string2b;
        
        string2a="hello";
        string2b=new String("hello");
        
        System.out.println("are the second set the same and equal? " +(string2a==string2b)+" and "+(string2a.equals(string2b)));
    }
}
